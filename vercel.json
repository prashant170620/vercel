{
  "version": 2,
  "builds": [
    {
      "src": "Server.js",
      "use":"@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/",
      "dest": "Server.js"
    },
    {
      "handle": "filesystem"
    },
    {
      "src": "/",
      "status": 200,
      "dest": "/hello.html"
    },
    {
      "src": "/(.*)",
      "status": 404,
      "dest": "/404.html"
    }
  ]
}
